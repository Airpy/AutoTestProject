<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- mysql连接数据库jar包存放地址-->
    <classPathEntry
            location="E:/idea_workspace/AutoTestProject/src/main/resources/mybatis/mysql-connector-java-6.0.5.jar"/>
    <context id="testTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true:是;false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.1.206:3306/autotest_rent_000" userId="root"
                        password="qazWSX098">
        </jdbcConnection>
        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和 NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成model类存放位置 -->
        <javaModelGenerator targetPackage="com.amio.automation.jianshu.dto.model" targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成mapper映射文件的位置
               如果maven工程只是单独的一个工程，targetProject="src/main/java"
               若果maven工程是分模块的工程，targetProject="所属模块的名称"，例如：
               targetProject="ecps-manager-mapper"，下同-->
        <sqlMapGenerator targetPackage="com.amio.automation.jianshu.dto.mapping" targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!-- 生成dao类存放位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.amio.automation.jianshu.dto.dao"
                             targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>
        <!-- 指定数据库表 -->
        <table schema="" tableName="T_BILL_SALE" domainObjectName="BillSale"/>
    </context>
</generatorConfiguration>